<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=listTitle %></h1>
    </div>
    <div class="box">
        <% addItem.forEach(element => { %>
            <div class="item">
                <input type="checkbox">
                <p><%= element%></p>
            </div>
            <% }); %>
                <form class="item" action="/" method="post">
                    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="work" value=<%= listTitle %>>+</button>
                </form>
            </div>
            <%-include("footer")-%>